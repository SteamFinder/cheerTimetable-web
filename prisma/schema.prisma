generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "mysql"
    url                  = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}

model Cookie {
    id    Int     @id @default(autoincrement())
    value String
    date  DateTime @default("2020-03-19T14:21:00+02:00")
}
